TARGET_MCU = STM32F103RC

BOARD_DEFINES += \
 -DUSE_USART_1 \
 -DGPIO_USART1_TX_PORT=GPIOB \
 -DGPIO_USART1_TX_PIN=6 \
 -DGPIO_USART1_TX_MODE=GPIO_Mode_AF_PP \
 -DGPIO_USART1_REMAP=1 \
 -DGPIO_USART1_RX_PORT=GPIOB \
 -DGPIO_USART1_RX_PIN=7 \
 -DGPIO_USART1_RX_MODE=GPIO_Mode_AF_PP \
 
 -DGPIO_LED1_PORT=GPIOB \
 -DGPIO_LED1_PIN=2 \
 -DGPIO_LED1_MODE=GPIO_Mode_Out_PP \
 -DGPIO_LED1_TIMER=timer1 \
 -DGPIO_LED1_TIM_CH=TIM_Channel_1 \
 -DGPIO_LED1_REMAP=0

 -DGPIO_PORT_HU GPIOA \
 -DGPIO_PORT_HV GPIOA \
 -DGPIO_PORT_HW GPIOA \
 -DGPIO_PORT_LU GPIOA \
 -DGPIO_PORT_LV GPIOA \
 -DGPIO_PORT_LW GPIOB \

 -DGPIO_PIN_HU 8 \
 -DGPIO_PIN_HV 9 \
 -DGPIO_PIN_HW 10 \
 -DGPIO_PIN_LU 11 \
 -DGPIO_PIN_LV 12 \
 -DGPIO_PIN_LW 1 \

 -DGPIO_PORT_BEMF_U GPIOB \
 -DGPIO_PORT_BEMF_V GPIOB \
 -DGPIO_PORT_BEMF_W GPIOB \
 -DGPIO_PIN_BEMF_U 12 \
 -DGPIO_PIN_BEMF_V 14 \
 -DGPIO_PIN_BEMF_W 15 \

 -DGPIO_PORT_CURRENT_U GPIOA \
 -DGPIO_PORT_CURRENT_V GPIOA \
 -DGPIO_PORT_CURRENT_W GPIOA \
 -DGPIO_PIN_CURRENT_U 0 \
 -DGPIO_PIN_CURRENT_V 1 \
 -DGPIO_PIN_CURRENT_W 2 \

 -DGPIO_PORT_FILTERED_CURRENT_U GPIOA \
 -DGPIO_PORT_FILTERED_CURRENT_V GPIOB \
 -DGPIO_PORT_FILTERED_CURRENT_W GPIOB \
 -DGPIO_PIN_FILTERED_CURRENT_U 7 \
 -DGPIO_PIN_FILTERED_CURRENT_V 0 \
 -DGPIO_PIN_FILTERED_CURRENT_W 11 \
 