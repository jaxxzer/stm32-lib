#sbus 1/3 A2
#sport/fport 1/6 AND 2/11 A5 and B10
#inverted sport 2/12 B11
#ch4 1/11 A10
#ch3 1/10 A9
#ch2 1/12 A11
#ch1 1/9 A8

#green led 1/13 A12
#red led 1/12 A11

#dip 1 A12
#dip 2 A11

#nrst 3/15 C14
#chip select 2/13 B12
#MOSI 2/16 B15
#MISO 2/13 AND 2/15 B12 and B14
#CK 2/14 B13

#sport 2/11 B10

#buzzer 2/2 B1
#eeprom pin1 2/8
TARGET_MCU = STM32F103CB

STARTUP = stm32f1
BOARD_DEFINES += \
  -DGPIO_LED1_PORT=GPIOA \
  -DGPIO_LED1_PIN=12 \
  -DUSE_USART_1 \
  -DSTDOUT_USART=uart1 \
  -DGPIO_USART1_TX_PORT=GPIOA \
  -DGPIO_USART1_TX_PIN=9 \
  -DGPIO_USART1_TX_MODE=GPIO_Mode_AF \
  -DGPIO_USART1_REMAP=1 \
  -DGPIO_USART1_RX_PORT=GPIOA \
  -DGPIO_USART1_RX_PIN=10 \
  -DGPIO_USART1_RX_MODE=GPIO_Mode_AF \
  -DGPIO_NRST_PORT=GPIOC \
  -DGPIO_NRST_PIN=14 \
  -DGPIO_PPM_PORT=GPIOA \
  -DGPIO_PPM_PIN=6 \
